{
    "class": "Telemetry",
    "My_System": {
        "class": "Telemetry_System",
        "systemPoller": {
            "interval": 60
        }
    },
    "My_Listener": {
        "class": "Telemetry_Listener",
        "port": 6514
    },
    "My_Consumer": {
        "class": "Telemetry_Consumer",
        "type": "AWS_CloudWatch",
        "dataType": "metrics",
        "metricNamespace": "f5_testing_namespace",
        "region": "$region",
        "customOpts": [
          {
             "customDeploymentName": "$deploymentName",
             "customEnvironment": "$environment",
             "customInstanceId": "$instanceId"
          }
        ]
    }
}
