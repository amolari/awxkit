{
    "schemaVersion": "1.17.0",
    "class": "Device",
    "async": true,
    "label": "first test for declarative onboarding",
    "controls": {
        "trace": true,
        "traceResponse": true
    },
    "Common": {
        "class": "Tenant",
        "mySystem": {
            "class": "System",
            "hostname": "$hostname",
            "cliInactivityTimeout": 1200,
            "consoleInactivityTimeout": 1200,
            "autoPhonehome": false
        },
        "myLicense": {
            "class": "License",
            "licenseType": "licensePool",
            "bigIqHost": "$bigIqAddress",
            "bigIqUsername": "$bigIqUsername",
            "bigIqPasswordUri": "$bigIqPasswordUri",
            "licensePool": "$bigIqLicensePoolName",
            "reachable": false,
            "hypervisor": "aws",
            "unitOfMeasure": "yearly",
            "tenant": "$bigIqLicenseTenant"
        },
        "myNtp": {
            "class": "NTP",
            "timezone": "$timezone"
        },
        "myProvisioning": {
            "class": "Provision",
            "ltm": "nominal",
            "asm": "nominal"
        },
        "localRoute": {
            "class": "Route",
            "gw": "$DEF_GW",
            "network": "default",
            "localOnly": true
        },
        "sshSettings": {
            "class": "SSHD",
            "allow": "all",
            "inactivityTimeout": 300,
            "banner": "WARNING\\nThis system is for the use of Richemont authorized users only.\\nIf you are not authorized, leave this system immediately.\\n"
        },
        "globalSysSettings": {
            "class": "System",
            "autoCheck": false,
            "autoPhonehome": false,
            "cliInactivityTimeout": 300,
            "consoleInactivityTimeout": 300,
            "guiAuditLog": true,
            "tmshAuditLog": true
        },
        "SnmpAgent": {
            "class": "SnmpAgent",
            "contact": "$snmpSysContact",
            "location": "$region"
        },
        "dbvars": {
            "class": "DbVariables",
            "tm.tcpudptxchecksum": "Software-only",
            "liveupdate.autodownload": "disable",
            "ui.advisory.enabled": true,
            "ui.advisory.color": "green",
            "ui.advisory.text": "DeploymentName: $deploymentName - EC2-ID: $instanceId - $availabilityZone",
            "ui.is24hour": true
        }
    }
}
